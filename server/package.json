{
  "name": "stranger-server",
  "version": "0.0.1",
  "description": "Stranger backend built using Expressjs",
  "main": "app.js",
  "scripts": {
    "cleanup": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "build:watch": "tsc --watch",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev src/app.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/app.js",
    "start": "cross-env NODE_ENV=production node dist/app.js"
  },
  "keywords": [
    "languages-spoken",
    "typescript"
  ],
  "author": "frostzt",
  "license": "MIT",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.0.1",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.3.2",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cookies": "^0.7.7",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frostzt/stranger-server.git"
  },
  "bugs": {
    "url": "https://github.com/frostzt/stranger-server/issues"
  },
  "homepage": "https://github.com/frostzt/stranger-server#readme"
}
